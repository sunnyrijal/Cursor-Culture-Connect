{
  "expo": {
    "name": "TRiVO",
    "slug": "bolt-expo-nativewind",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "myapp",
    "owner": "rijalboy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/logo.png",        
      "resizeMode": "contain",             
      "backgroundColor": "#ffffff",        
      "dark": {
        "image": "./assets/logo.png",
        "backgroundColor": "#000000"
      }
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trivo.connect"
    },
    "android": {
      "package": "com.trivo.connect"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fecb2cb4-c2cd-4536-a6c8-fea3c961c70c"
      }
    }
  }
}
