{
  "expo": {
    "name": "TRiVO",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/logo.png",        
      "resizeMode": "contain",             
      "backgroundColor": "#ffffff",        
      "dark": {
        "image": "./assets/logo.png",
        "backgroundColor": "#000000"
      }
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.babish.dev.boltexponativewind"
    },
    "android": {
      "package": "com.babish.dev.boltexponativewind"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a417af0b-0523-456b-bd77-df807a7c19a8"
      }
    },
    "owner": "babish9887"
  }
}
